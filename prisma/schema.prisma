// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Gender{
  M 
  F 
  Other
}

model User{
  
  first_name String
  last_name String
  phone String @unique
  password String
  salt String
  avatar String?
  email String? @unique
  address String?
  gender Gender? 
  sessionToken String?
  id String @id @default(uuid())

  @@map("users")
}
